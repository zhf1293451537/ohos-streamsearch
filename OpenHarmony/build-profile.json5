{
  "app": {
    "products": [
      {
        "name": "default",
        "signingConfig": "default",
        "compileSdkVersion": 13,
        "compatibleSdkVersion": 13,
        "targetSdkVersion": 13,
        "runtimeOS": "OpenHarmony"
        "buildOption": {
          "strictMode": {
            "caseSensitiveCheck": true,
            "useNormalizedOHMUrl": true
          }
        }
      }
